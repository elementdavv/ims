<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.i18n.encoding" value="UTF-8"/>
    <!-- constant name="struts.multipart.maxSize" value="1000000000"/-->
	
	<include file="struts-default.xml" />
    <package name="com.sealtalk.action" extends="struts-default">
        <action name="system" class="com.sealtalk.action.SystemAction">
           	<result name="loginPage">/page/system/login.jsp</result>
			<result name="loginSuccess" type="redirect">/page/mainFrame.jsp</result>
        </action>
    </package>

</struts>
