<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sealtalk.auth.model.AppSecret" table="t_appsecret" catalog="tfdb">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="appId" type="java.lang.String">
            <column name="appid" not-null="true" length="20" />
        </property>
        <property name="secert" type="java.lang.String">
            <column name="secert" not-null="true" length="100" />
        </property>
        <property name="callBackUrl" type="java.lang.String">
            <column name="callbackurl" length="256" />
        </property>
        <property name="time" type="java.lang.Long">
            <column name="time" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
