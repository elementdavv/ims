<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sealtalk.auth.model.UserValid" table="t_uservalid" catalog="tfdb">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="asid" type="java.lang.Integer">
            <column name="asid" />
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="userid" />
        </property>
        <property name="info" type="java.lang.Integer">
            <column name="info" />
        </property>
        <property name="unAuthToken" type="java.lang.String">
            <column name="unauthtoken"  length="100" />
        </property>
        <property name="authToken" type="java.lang.String">
            <column name="authtoken" length="100" />
        </property>
        <property name="visitToken" type="java.lang.String">
            <column name="visittoken" length="100" />
        </property>
        <property name="unAuthTokenTime" type="java.lang.Long">
            <column name="unauthtokentime"  />
        </property>
        <property name="authTokenTime" type="java.lang.Long">
            <column name="authtokentime"  />
        </property>
        <property name="visitTokenTime" type="java.lang.Long">
            <column name="visittokentime" />
        </property>
    </class>
</hibernate-mapping>
