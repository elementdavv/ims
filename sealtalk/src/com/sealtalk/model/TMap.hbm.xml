<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sealtalk.model.TMap" table="t_map" catalog="tfdb">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" not-null="true" />
        </property>
        <property name="latitude" type="java.lang.String">
            <column name="latitude" not-null="true" />
        </property>
        <property name="longitude" type="java.lang.String">
            <column name="longitude" not-null="true" />
        </property>
        <property name="subDate" type="java.lang.Long">
            <column name="subdate" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
