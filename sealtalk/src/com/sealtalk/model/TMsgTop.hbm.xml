<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sealtalk.model.TMsgtop" table="t_msgtop" catalog="tfdb">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" not-null="true" />
        </property>
        <property name="topId" type="java.lang.Integer">
            <column name="top_id" length="11" />
        </property>
        <property name="msgType" type="java.lang.String">
            <column name="msg_type" not-null="true" />
        </property>
        <property name="listorder" type="java.lang.Integer">
            <column name="listorder" length="11" />
        </property>
    </class>
</hibernate-mapping>
