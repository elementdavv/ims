<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <!--<package name="com.sealtalk.action.msg" extends="ssl-default">-->
    <package name="com.sealtalk.action.msg" extends="struts-default">
   		<!-- 消息管理 -->
        <action name="messageAction" class="com.sealtalk.action.msg.MessageAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
        </action>
        <!-- 用户服务 -->
        <action name="userServiceAction" class="com.sealtalk.action.msg.UserServiceAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
        </action>
    </package>
   
</struts>

